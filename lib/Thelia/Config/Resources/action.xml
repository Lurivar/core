        <service id="thelia.action.newsletter" class="Thelia\Action\Newsletter">
            <argument type="service" id="service_container"/>
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="thelia.action.lang" class="Thelia\Action\Lang">
            <argument type="service" id="service_container"/>
            <tag name="kernel.event_subscriber"/>
        </service>
